{
    "name": "seodably/seo",
    "description": "Un package SEO avancé pour Laravel avec analyse en temps réel, IA et interface Vue.js/Inertia.js",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SABOWA Ryan",
            "email": "contact@sabowaryan.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "inertiajs/inertia-laravel": "^1.0",
        "guzzlehttp/guzzle": "^7.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.3",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Seodably\\SEO\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seodably\\SEO\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seodably\\SEO\\SEOServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 